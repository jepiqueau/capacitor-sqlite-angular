{
  "name": "sqlite-ionic",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "npm run prepare && ng serve -o",
    "serve": "npm run start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "npm run copy:sqlite:wasm",
    "copy:sqlite:wasm": "cp node_modules/sql.js/dist/sql-wasm.wasm src/assets/sql-wasm.wasm",
    "electron:start": "rm -f src/assets/sql-wasm.wasm && npm run build &&  npx cap copy @capacitor-community/electron && cd electron && npm run electron:start"
  },
  "private": true,
